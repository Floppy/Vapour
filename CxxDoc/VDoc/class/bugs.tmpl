$(//:#===--- Top )
$(include:top.stmpl)

<TABLE WIDTH="100%" BORDER=0 CELLPADDING=0 CELLSPACING=4>
	<TR VALIGN="TOP" WIDTH="10%">

$(//:#===--- Menu)
$(include:menu.stmpl)

	<!-- Main Start -->
	<TD WIDTH="$(tablewidth)">
	<TABLE WIDTH="100%" BORDER="0" CELLPADDING="2" CELLSPACING="0" CLASS="ITEMBORDER" BGCOLOR="#000080">
		<TR><TD>
		<P CLASS="SECTIONTITLE">
		Bug List
		</P>
		</TD></TR>
		<TR><TD>
		<TABLE WIDTH="100%" BORDER="0" CELLPADDING="2" CELLSPACING="0" CLASS="ITEM" BGCOLOR="#FFFFFF">
			<TR><TD>
			
			<P><B>Select Architecture</B></P>
			<?php
				$doc_path = "$(docpath)";
				include_once("vsrcdoc_core.php");
				
				make_arch_bar("bugs.php");
			?>

			<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="2">
			<TR><TD BGCOLOR="#C0C0C0"><P><B>Class</B></P></TD><TD BGCOLOR="#C0C0C0"><P><B>Method</B></P></TD><TD BGCOLOR="#C0C0C0"><P><B>Description</B></P></TD></TR>
			$(forall:class:
				$(ifset:classbug:
					<TR VALIGN="TOP"><TD COLSPAN="2" BGCOLOR="#C0C0F0"><P>Class <A HREF="$(docroot)/$(MakeFilename:developer/class/%.php)">$(ObjectName)</A></P></TD><TD BGCOLOR="#C0C0F0"><P>$(list:$(classbug))</P></TD></TR>
				)
				$(set:inclass=$(ObjectName))$(set:infile=$docroot/$(MakeFilename:../developer/class/%.php))
				$(forall:method:
					$(ifset:bug:
						<TR VALIGN="TOP"><TD BGCOLOR="#C0C0F0"><P><A HREF="$(infile)">$(inclass)</A></P></TD><TD BGCOLOR="#C0C0F0"><P><A HREF="$(infile)#$(href)">$(ObjectName)</A></P></TD><TD BGCOLOR="#C0C0F0"><P>$(list:$(bug))</P></TD></TR>
					)
				)
			)
			</TABLE>
			</TD></TR>
		</TABLE>
		</TD></TR>
	</TABLE>
	</TD>
